{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*.astro",
  ],
  "exclude": [
    "build"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "source/components/*"
      ],
      "@layouts/*": [
        "source/layouts/*"
      ],
      "@pages/*": [
        "source/pages/*"
      ],
      "@utils/*": [
        "source/utilities/*"
      ],
      "@images/*": [
        "source/assets/images/*"
      ],
      "@fonts/*": [
        "source/assets/fonts/*"
      ]
    }
  }
}